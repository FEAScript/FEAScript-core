{
  "name": "feascript",
  "version": "0.2.0",
  "description": "Lightweight finite element simulation library built in JavaScript",
  "main": "dist/feascript.cjs.js",
  "module": "dist/feascript.esm.js",
  "browser": "dist/feascript.umd.js",
  "exports": {
    "import": "./dist/feascript.esm.js",
    "require": "./dist/feascript.cjs.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "for file in $(find examples -name '*.js' -type f | sort); do echo 'Running' \"$file\"; if node \"$file\" > /dev/null 2>&1; then echo 'Success'; else echo 'Failed'; fi; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FEAScript/FEAScript-core.git"
  },
  "keywords": [
    "finite element",
    "simulation",
    "javascript",
    "FEA",
    "FEM",
    "finite element analysis",
    "finite element method"
  ],
  "author": "Nikolaos Chamakos <nikoscham@gmail.com> (https://www.npmjs.com/~nikoscham)",
  "contributors": [
    {
      "name": "sridhar-mani",
      "url": "https://www.npmjs.com/~sridhar-mani"
    },
    {
      "name": "Felipe Ferrari",
      "url": "https://github.com/ferrari212"
    },
    {
      "name": "Akhilesh Tiwari",
      "url": "https://github.com/silky-x0"
    },
    {
      "name": "Priyank Bansal",
      "url": "https://github.com/Stu-ops"
    }
  ],
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/FEAScript/FEAScript-core/issues"
  },
  "homepage": "https://feascript.com/",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "mathjs": "^11.12.0",
    "plotly.js": "^2.35.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "mathjs": "^11.12.0",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-wasm": "^3.0.0",
    "taichi.js": "^0.0.36"
  }
}
